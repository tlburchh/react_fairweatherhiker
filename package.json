{
  "name": "react-map-gl-geocoder-example",
  "version": "1.0.0",
  "description": "\nExample usage of react-map-gl-geocoder which is  a React wrapper for mapbox-gl-geocoder for use with react-map-gl",
  "keywords": [
    "react",
    "mapbox-gl",
    "mapbox-gl-geocoder",
    "react-map-gl",
    "mapbox"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^2.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-map-gl": "3.2.10",
    "react-map-gl-geocoder": "1.3.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-local": "webpack-dev-server --env.local --progress --hot --open",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node server/scripts/seedDB.js && node server/scripts/seedAssociations.js",
    "seed2": "node server/scripts/seedAssociations.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.x",
    "yarn": "1.10.1"
  }
}
